{
   "name": "rtk-slice-manager",
   "version": "0.0.3",
   "description": "Wrap over slice from @reduxjs/toolkit for automatic generation of actions and isolation of effect logic after changing the state of the fields",
   "main": "dist/index.js",
   "types": "dist",
   "maintainers": [
      {
         "name": "Dmitry Osincev"
      }
   ],
   "scripts": {
      "build": "npm run clean && tsc -b",
      "clean": "rm -rf ./dist",
      "prepublish": "npm run build && npm run test",
      "test": "jest"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Ynhito/SliceManager.git"
   },
   "keywords": [
      "rtk",
      "slice",
      "manager",
      "effects",
      "watcher",
      "redux",
      "react",
      "hook",
      "store"
   ],
   "author": "Dmitry Osincev",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/Ynhito/SliceManager/issues"
   },
   "homepage": "https://github.com/Ynhito/SliceManager#readme",
   "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "verbose": true
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm test",
         "...": "..."
      }
   },
   "peerDependencies": {
      "redux": "4.1.2",
      "redux-thunk": "2.4.1",
      "@reduxjs/toolkit": "1.6.2",
      "react": "17.0.2",
      "@types/react": "17.0.3",
      "react-redux": "7.2.6"
   },
   "dependencies": {},
   "devDependencies": {
      "@types/jest": "25.2.3",
      "@types/node": "14.0.6",
      "babel-jest": "^27.4.2",
      "husky": "^4.2.5",
      "install-peers": "1.0.3",
      "jest": "^26.0.1",
      "ts-jest": "26.0.0",
      "typescript": "4.5.2"
   }
}
